{
    "manifest_version": 3,
    "name": "Radon",
    "version": "1.0",
    "description": "Make Chrome Better",
    "host_permissions": ["*://*/*"],
    "action": {
      "default_popup": "src/popup.html"
    },

    "chrome_url_overrides": {
      "newtab": "src/newtab.html"
    },
    "permissions": [
      "storage"
    ],
  "icons": {
    "128": "src/imgs/ico.png"
  }
}
  